{
  "name": "mrbidouille",
  "version": "1.0.0",
  "description": "Mr. Bidouille Static Blog",
  "author": "Tim Winkelmann",
  "private": true,
  "scripts": {
    "dev": "cross-env NUXT_HOST=0.0.0.0 nuxt",
    "build": "nuxt build",
    "start": "cross-env NUXT_HOST=0.0.0.0 nuxt start",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "reformat": "prettier \"**/*.{js,json,html,vue}\" --write"
  },
  "dependencies": {
    "nuxt": "^2.14.12"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "lodash": "^4.17.20",
    "node-sass": "^5.0.0",
    "nuxt-purgecss": "^1.0.0",
    "prettier": "2.1.2",
    "push-dir": "^0.4.1",
    "sass-loader": "^10.1.0",
    "tailwindcss": "^1.0.0"
  }
}
